string=""
def removePunctuation(string):
    punctuation = "!@#$%^&*()+="
    str=""
    for char in string:
        if char not in punctuation:
            str+=char
    return str
def count_vowels(string):
    count=0
    for char in string:
        if (char=='a' or char=='e' or char=='i' or char=='o' or char=='u' or char=='A' or char=='E' or char=='I' or char=='O' or char=='U'):
            count+=1
    return count
